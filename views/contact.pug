extends base.pug


block scripts
  script(src='/static/index.js')
  
block style
    style
      include ../static/style.css
      include ../static/stylecontact.css
block content
    div.container
        h1 Contact Us
        form(action="/contact", method="post",class="myForm")
            input(type="text",class="myInput",name="name",placeholder='Enter your name')
            input(type="date",class="myInput",name="date",placeholder='Enter your DOB')
            input(type="phone",class="myInput",name="phoone",placeholder='Enter your phonenumber')
            input(type="email",class="myInput",name="email",placeholder='Enter your emailId')
            input(type="address",class="myInput",name="address",placeholder='Enter your address')
            button(class="btn")
             | Submit
